{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/login",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/register",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/*",
            "allowedRoles": [ "anonymous" ]
        }
    ],
    "auth": {
        "rolesSource": "/api/roles",
        "identityProviders": {
            "github": {
                "registration": {
                    "clientIdSettingName": "GITHUB_CLIENT_ID",
                    "clientSecretSettingName": "GITHUB_CLIENT_SECRET_APP_SETTING_NAME"
                }
            }
        }
    }
}
