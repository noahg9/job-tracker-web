{
    "routes": [
        {
            "route": "/api/user/login",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/api/user/register",
            "allowedRoles": [ "anonymous" ]
        },
        {
            "route": "/api/*",
            "allowedRoles": [ "authenticated" ]
        },
        {
            "route": "/*",
            "allowedRoles": [ "authenticated" ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}
